<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Show Vendors</title>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
    <div class="container">
        <h2 class="mt-4">Vendor List</h2>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Mobile Number</th>
                    <th>Email</th>
                    <th>Username</th>
                    <th>Address</th>
                    <th>Pincode</th>
                    <th>City</th>
                </tr>
            </thead>
            <tbody>
                {{#each vendors}}
                <tr>
                    <td>{{this.fname}}</td>
                    <td>{{this.lname}}</td>
                    <td>{{this.mobno}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.username}}</td>
                    <td>{{this.adress}}</td>
                    <td>{{this.pincode}}</td>
                    <td>{{this.city}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</body>
</html>
